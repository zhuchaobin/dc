		t2.GROUP_ID_,
		t1.TASK_DEF_KEY_
	FROM
		act_ru_task t1,
		act_ru_identitylink t2
	WHERE
		t1.ID_ = t2.TASK_ID_
	AND t2.GROUP_ID_ = '01'
) t4 ON t6.PROCESS_INST_ID = t4.PROC_INST_ID_

